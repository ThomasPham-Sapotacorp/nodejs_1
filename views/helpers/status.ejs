<% 
    statusHelper = (type, status, id) => {
        let link = systemConfig.prefixAdmin + '/'+type+'/change-status/' + id + '/' + status;
        let xhtmlStatus = '';
        if(status == 'active') {
            xhtmlStatus = '<span class="label label-success">active</span>';
        }else if(status == 'inactive') {
            xhtmlStatus = '<span class="label label-default">inactive</span>';
        }
        return `<a href="${link}">${xhtmlStatus}</a>`;
    }
%>
